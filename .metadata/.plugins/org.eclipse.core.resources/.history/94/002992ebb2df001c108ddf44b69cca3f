package Member;

import java.util.*;

public class MemberServise {

	static MemberVO[] members;

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);

		members = new MemberVO[0];
		boolean isStop = false;

		do {
			System.out.println("다음 메뉴중 하나를 고르세요.");
			System.out.println("1.");
			System.out.println("2.");
			System.out.println("3.");
			System.out.println("4.");
			System.out.println("5.");
			System.out.println("종료.");

			String command = sc.next();

			switch (command) {

			case "1":
				AddAction WriteAction = new AddAction();
				break;
			case "2":
				break;
			case "3":
				break;
			case "4":
				break;
			case "5":
				isStop = true;

			}
		} while (!isStop);
	}

	void process(Action action, Scanner sc) {
		action.equals(sc);// 인터페이스의 다형성 이용

	}

}
