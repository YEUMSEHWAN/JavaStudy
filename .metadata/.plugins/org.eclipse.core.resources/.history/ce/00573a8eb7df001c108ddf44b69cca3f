package Member;

import java.util.Scanner;

public class ListAction implements Action {



	@Override
	public void excute(Scanner sc) {
		MemberVO[]members = MemberMain.members;
		if(members.length>=1) {
			
			for(int i = 0; i<members.length;i++) {
				System.out.println("이름 : " + members[i].getName()+",전화번호 : "+members[i]
						+",국적 : "+members[i].getNation()+", 나이 : "+members[i].getAge()+", 이메일 : "+members[i].getEmail());
			}
		}
		
	}
	}


